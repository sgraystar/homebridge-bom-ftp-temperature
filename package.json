    "@typescript-eslint/parser": "^5.0.0",
    "basic-ftp": "^5.0.2",
    "eslint": "^8.0.1",
    "fast-xml-parser": "^4.0.11",
    "homebridge": "^1.3.5",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
