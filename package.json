{
  "private": false,
  "displayName": "Homebridge BoM FTP Temperature",
  "name": "homebridge-bom-ftp-temperature",
  "version": "0.1.0",
  "description": "The forecast maximum temperature is downloaded from BoM FTP Public Products and presented as a temperature sensor.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sgraystar/homebridge-bom-ftp-temperature.git"
  },
  "bugs": {
    "url": "https://github.com/sgraystar/homebridge-bom-ftp-temperature/issues"
  },
  "engines": {
    "node": ">=14.18.1",
    "homebridge": ">=1.3.5"
  },
  "main": "dist/index.js",
  "scripts": {
